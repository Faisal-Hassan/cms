<%= link_to "Back", pages_path(:subject_id => @subject_id), :class => "Back-link" %>

<div class="pages new">
  <h2>Create Page</h2>
  
  <%= form_for(@page, :url => pages_path(:subject_id => @subject_id) do |f| %> 
    
    <%= render(:partial => 'form', :locals => {:f => f}) %>
    
    <div class="form-buttons">
      <%= f.submit "Create Page" %>
    </div>
    
  <% end %>
</div>