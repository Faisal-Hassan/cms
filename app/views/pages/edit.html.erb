<%= link_to "Back", pages_path(:subject_id => @subject_id), :class => "Back-link" %>

<div class="pages edit">
  <h2>Update Page</h2>
  
  <%= form_for(@page, :url => page_path(@page, :subject_id => @subject_id)) do |f| %> 
    
    <%= render(:partial => 'form', :locals => {:f => f}) %>
    
    <div class="form-buttons">
      <%= f.submit "Update Page" %>
    </div>
    
    
  <% end %>
</div>